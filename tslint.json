{
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "extends": [
    "tslint-eslint-rules",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-unused-expression": true,
    "newline-before-return": true,
    "jsdoc-format": false,
    "component-class-suffix": [true, "Component", "View"]
  }
}
